var requestId = <%= @request.id  %>
var request = $("#request_"+requestId)
var requests_table = $("#requests_table").DataTable()
var row = requests_table.row(request)

row.child( "<%= j render partial: 'requests/details_table' %>" ).show();
request.find(".details_link").html("<i class='material-icons'>remove</i>")
request.addClass('shown');

Materialize.updateTextFields();
